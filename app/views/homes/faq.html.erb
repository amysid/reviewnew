<div class="page-wrapper">
      <div class="faq-page">
        <div class="container">
          <h1 class="page-title">FAQs</h1>
          <div class="accordion" id="accordionExample">
            <% @faqs&.each do |faq| %>
            <div class="card">
              <div class="card-header accordion-card" id="headingOne" data-toggle="collapse"
                data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne"> <%= faq&.question %>
              </div>
              <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordionExample">
                <div class="card-body">
                  <%= faq&.answer%>
                </div>
              </div>
            </div>
            <%end%>
          </div>
        </div>
      </div>
    </div>